{
  "name": "forkgen-server",
  "version": "0.0.1",
  "description": "A web (localhost) based open source tool for GUI for Git that allows you control Git without using the command line.",
  "scripts": {
    "start": "babel-node ./src/server.js",
    "dev": "webpack -d --watch",
    "build": "webpack -p",
    "test": "jest --no-cache",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/forkgen/forkgen-server"
  },
  "keywords": [
    "Git",
    "Github",
    "BitBucket",
    "versioning",
    "distributed"
  ],
  "author": "Ashwin Hegde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forkgen/forkgen-server/issues"
  },
  "homepage": "https://github.com/forkgen/forkgen-server",
  "dependencies": {
    "body-parser": "~1.15.0",
    "bunyan": "^1.8.5",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "es6-promise": "^3.2.1",
    "events": "^1.1.0",
    "express": "~4.13.1",
    "express-session": "^1.14.2",
    "helmet": "^3.3.0",
    "joi": "^8.0.5",
    "node-dir": "^0.1.10",
    "nodegit": "^0.16.0",
    "nodemailer": "^2.7.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.2",
    "object-assign": "^4.1.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.6.0",
    "superagent": "^1.8.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^7.0.0"
  }
}
